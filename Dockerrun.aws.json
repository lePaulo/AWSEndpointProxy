{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "squid-conf",
      "host": {
        "sourcePath": "/var/app/current/squid.conf"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "squid",
      "image": "sameersbn/squid:3.3.8-23",
      "memory":"512",
      "essential": true,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-squid",
          "containerPath": "/var/log/squid3"
        },
        {
          "sourceVolume": "squid-conf",
          "containerPath": "/etc/squid3/squid.conf"
        }
      ],
      "portMappings": [
        {
          "hostPort": 443,
          "containerPort": 3128
        },
        {
          "hostPort": 80,
          "containerPort": 3128
        }
      ]
    }
  ]
}
